package collection

import (
	"reflect"
	"testing"
)

func TestQueueAPI(t *testing.T) {
	//1. 新建一个队列
	q := InitQueue[int]()

	//2. 向队列加入元素
	q.Push(100)
	q.Push(79)
	q.Push(38)

	//判断长度是否为3
	if q.Len() != 3 {
		t.Fatalf("unexpected length of queue.")
	}

	if !reflect.DeepEqual(q.Traverse(), []int{100, 79, 38}) {
		t.Fatalf("unexpected Traverse!")
	}

	if q.Front() != 100 {
		t.Fatalf("unexpected first value.")
	}
	if q.Back() != 38 {
		t.Fatalf("unexpected last value.")
	}

	//弹出元素
	val := q.Pop()
	if val != 100 {
		t.Fatalf("unexpected value.")
	}

	//判断第一个元素与最后一个元素
	if q.Front() != 79 {
		t.Fatalf("unexpected first value.")
	}
	if q.Back() != 38 {
		t.Fatalf("unexpected last value.")
	}

	val = q.Pop()
	if val != 79 {
		t.Fatalf("unexpected value.")
	}

	//判断长度是否为1
	if q.Len() != 1 {
		t.Fatalf("unexpected length of queue.")
	}

	if q.Front() != 38 {
		t.Fatalf("unexpected first value.")
	}
	if q.Back() != 38 {
		t.Fatalf("unexpected last value.")
	}

	//清空元素
	q.Clear()
	if !q.Empty() {
		t.Fatalf("expected empty.")
	}

}
